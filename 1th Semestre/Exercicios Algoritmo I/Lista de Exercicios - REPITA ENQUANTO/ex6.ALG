algoritmo "semnome"
var
   op,n:inteiro
inicio
      op<-0
      escreva("digite um numero qualquer:")
      leia(n)
      enquanto n>0 faca
               op<-op+1
               escreva("Digite Um numero qualquer:")
               leia(n)
      fimenquanto
      escreva("A quantidade de numero digitados foram:", op)

fimalgoritmo